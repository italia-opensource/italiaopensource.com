{
  "name": "italiaopensource.com",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "typecheck": "tsc",
    "mdx-checker": "npx docusaurus-mdx-checker",
