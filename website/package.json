    "@docusaurus/preset-classic": "^3.5.2",
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@formspree/react": "^2.3.3",
    "@mdx-js/react": "^3.0.0",
    "@mui/icons-material": "^5.11.0",
    "@mui/material": "^5.11.8",
    "@mui/x-charts": "^6.18.2",
