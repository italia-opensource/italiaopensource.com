{
  "name": "italiaopensource.com",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "typecheck": "tsc",
    "mdx-checker": "npx docusaurus-mdx-checker",
    "create-landings": "./scripts/create-landings.sh",
    "create-pages": "./scripts/create-pages.sh",
    "setup": "chmod +x scripts/*.sh && ./scripts/setup.sh",
    "download": "./scripts/download.sh",
    "build": "yarn create-landings && yarn create-pages && yarn mdx-checker && yarn docusaurus build",
    "clear": "yarn docusaurus clear",
    "serve": "yarn docusaurus serve",
    "start": "yarn docusaurus start",
    "deploy": "./scripts/deploy.sh",
    "lint": "eslint . --fix",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,md,json}\"",
    "prepare": "husky"
  },
  "dependencies": {
    "@docusaurus/core": "^3.6.3",
    "@docusaurus/faster": "^3.6.3",
    "@docusaurus/plugin-google-gtag": "^3.6.3",
    "@docusaurus/plugin-pwa": "^3.6.3",
    "@docusaurus/plugin-rsdoctor": "^3.6.3",
    "@docusaurus/plugin-sitemap": "^3.6.3",
    "@docusaurus/preset-classic": "^3.6.3",
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@formspree/react": "^2.3.3",
    "@mdx-js/react": "^3.0.0",
    "@mui/icons-material": "^6.1.4",
    "@mui/lab": "^6.0.0-beta.12",
    "@mui/material": "^6.1.4",
    "@mui/x-charts": "^6.18.2",
    "@pankod/refine-core": "^3.99.4",
    "@pankod/refine-react-router-v6": "^3.38.0",
    "@pankod/refine-simple-rest": "^3.37.4",
    "@types/classnames": "^2.2.10",
    "@types/react": "^18.2.0",
    "@types/react-transition-group": "^4.2.4",
    "clsx": "^1.2.1",
    "framer-motion": "4.1.17",
    "leaflet": "^1.9.1",
    "prettier": "^3.3.3",
    "primeflex": "^3.2.1",
    "primeicons": "^5.0.0",
    "primereact": "^8.4.0",
    "prism-react-renderer": "^2.2.0",
    "react": "^18.2.0",
    "react-countup": "^6.5.0",
    "react-device-detect": "^2.2.3",
    "react-dom": "^18.2.0",
    "react-leaflet": "^3.0.0",
    "react-scripts": "^5.0.1",
    "react-transition-group": "^4.4.1"
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "^3.6.3",
    "@docusaurus/tsconfig": "^3.6.3",
    "@eslint/js": "^9.22.0",
    "@tsconfig/docusaurus": "^1.0.5",
    "@types/leaflet": "^1.8.0",
    "@types/node": "^18.7.23",
    "@types/react": "^18.2.0",
    "eslint-plugin-markdown": "^5.1.0",
    "@typescript-eslint/eslint-plugin": "^8.10.0",
    "@typescript-eslint/parser": "^8.10.0",
    "eslint-config-prettier": "^10.1.1",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.0",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.26.1"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.md": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.json": [
      "prettier --write"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not op_mini all"
  ],
  "engines": {
    "node": ">=16.14"
  }
}
